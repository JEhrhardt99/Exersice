STAN <- read.csv2("STAN-All-1970-2006.csv", header = TRUE, sep = ",")
################################ Getting to know the Variables ################################
#View(STAN)
summary(factor(STAN$INDUSTRY))
summary(factor(STAN$WAGE))
summary(factor(STAN$COUNTRY))
summary(factor(STAN$VALU))
summary(factor(STAN$ISICREV3))
summary(factor(STAN$YEAR))
STAN <- STAN[order(STAN$COUNTRY, STAN$ISICREV3, STAN$YEAR),]
STAN_red <- STAN[c("COUNTRY", "YEAR", "VALU", "WAGE", "INDUSTRY", "ISICREV3")]
STAN_red <- STAN_red[order(STAN$COUNTRY, STAN$YEAR, STAN$VALU),]
STAN_red <- STAN_red[order(STAN_red$COUNTRY, STAN_red$INDUSTRY, STAN_red$YEAR),]
STAN_red$idnr <- STAN_red %>% group_indices(COUNTRY, INDUSTRY)
STAN_red <- STAN_red %>% group_by(idnr) %>% mutate(VALU_lag_1 = dplyr::lag(VALU, n = 1L, default = NA,))
STAN_red$VALU_gr <- (STAN_red$VALU - STAN_red$VALU_lag_1)/STAN_red$VALU_lag_1
View(STAN_red)
